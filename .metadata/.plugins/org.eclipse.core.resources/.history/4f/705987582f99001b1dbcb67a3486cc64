<body>
	<canvas id=canvas width="1000" height="1000" style="border: 5px solid black; margin: 20px 0px 0px 20px;"></canvas>
	<script>
	var canvas = document.getElementById("canvas");
	var ctx = canvas.getContext("2d");
	ctx.beginPath();
	
// 	가로1
	ctx.moveTo(470, 450);
	ctx.lineTo(530, 450);
	
// 	가로2
	ctx.moveTo(455, 480);
	ctx.lineTo(515, 480);

// 	가로3
	ctx.moveTo(455, 510);
	ctx.lineTo(515, 510);
	
// 	사선(/)1
	ctx.moveTo(470, 450);
	ctx.lineTo(455, 480);
	
// 	사선(/)2
	ctx.moveTo(530, 450);
	ctx.lineTo(515, 480);

// 	사선(/)3
	ctx.moveTo(530, 480);
	ctx.lineTo(515, 510);
	
// 	세로1
	ctx.moveTo(455, 480);
	ctx.lineTo(455, 510);
	
// 	세로2
	ctx.moveTo(515, 480);
	ctx.lineTo(515, 510);

// 	세로3
	ctx.moveTo(530, 450);
	ctx.lineTo(530, 480);
	

// 	가로1
	ctx.moveTo(470+30, 450-60);
	ctx.lineTo(530+30, 450-60);
	
// 	가로2
	ctx.moveTo(455, 480-60);
	ctx.lineTo(515, 480-60);

// 	가로3
	ctx.moveTo(455, 510-60);
	ctx.lineTo(515, 510-60);
	
// 	사선(/)1
	ctx.moveTo(470, 450-60);
	ctx.lineTo(455, 480-60);
	
// 	사선(/)2
	ctx.moveTo(530, 450-60);
	ctx.lineTo(515, 480-60);

// 	사선(/)3
	ctx.moveTo(530, 480-60);
	ctx.lineTo(515, 510-60);
	
// 	세로1
	ctx.moveTo(455, 480-60);
	ctx.lineTo(455, 510-60);
	
// 	세로2
	ctx.moveTo(515, 480-60);
	ctx.lineTo(515, 510-60);

// 	세로3
	ctx.moveTo(530, 450-60);
	ctx.lineTo(530, 480-60);
	
	
	ctx.stroke();
	</script>
</body>
